<!-- FLOT PIE PLUGIN - also used to draw donut charts -->
<script src="/plugin/flot/plugins/jquery.flot.pie.js"></script>
<script>
  $(function () {
    /*
     * DONUT CHART
     * -----------
     */

    var donutData = [
      {
        label: 'Lomba',
        data: 30,
        color: '#00909a',
      },
      {
        label: 'Event',
        data: 70,
        color: '#0c3c62',
      },
    ];
    $.plot('#kategoriEvent', donutData, {
      series: {
        pie: {
          show: true,
          radius: 1,
          innerRadius: 0.5,
          label: {
            show: true,
            radius: 2 / 3,
            formatter: labelFormatter,
            threshold: 0.1,
          },
        },
      },
      legend: {
        show: false,
      },
    });
    /*
     * END DONUT CHART
     */
  });

  /*
   * Custom Label formatter
   * ----------------------
   */
  function labelFormatter(label, series) {
    return (
      '<div style="font-size:13px; text-align:center; padding:2px; color: #fff; font-weight: 600;">' +
      label +
      '<br>' +
      Math.round(series.percent) +
      '%</div>'
    );
  }
</script>
